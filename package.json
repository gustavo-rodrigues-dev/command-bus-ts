{
  "name": "command-bus-ts-decorator",
  "version": "1.0.0",
  "description": "This project provide a simple and decoupled solution from a command handle structure, where you can use with decorators.",
  "repository": "https://github.com/gustavobeavis/command-bus-ts-decorator",
  "keywords": [
    "typescript",
    "commandbus",
    "decorator"
  ],
  "homepage": "https://github.com/gustavobeavis/command-bus-ts-decorator",
  "badges": [],
  "main": "./bin/index.js",
  "files": [
    "bin/**"
  ],
  "engines": {
    "node": ">=10.13.0"
  },
  "scripts": {
    "test": "npm run test:e2e",
    "test:e2e": "nyc mocha --opts ./test/e2e/.mocha.opts",
    "test:e2e:debug": "mocha --opts ./test/e2e/.mocha.opts --inspect-brk",
    "build": "tsc",
    "lint": "npm run lint:format && npm run lint:style",
    "lint:fix": "npm run lint:format:fix && npm run lint:style:fix",
    "lint:format": "prettier --check '{src,test}/**/*.ts'",
    "lint:format:fix": "prettier --write '{src,test}/**/*.ts'",
    "lint:style": "tslint --project .",
    "lint:style:fix": "tslint --project . --fix"
  },
  "author": "Gustavo S. Rodrigues <gustavo.beavis@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/chai-as-promised": "^7.1.2",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.11.1",
    "@types/sinon": "^9.0.0",
    "@types/sinon-chai": "^3.2.4",
    "@typescript-eslint/parser": "^2.27.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "mocha": "^7.1.1",
    "nyc": "^15.0.1",
    "prettier": "^2.0.4",
    "sinon": "^9.0.2",
    "sinon-chai": "^3.5.0",
    "sinon-chai-calls-assertion": "^2.0.0",
    "source-map-support": "^0.5.16",
    "ts-node": "^8.8.2",
    "tslint": "^6.1.1",
    "tslint-config-airbnb-base": "^0.3.0",
    "typedoc": "^0.17.4",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "@types/verror": "^1.10.3",
    "verror": "^1.10.0"
  }
}
